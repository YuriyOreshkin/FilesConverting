<!DOCTYPE html>
<html> 
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title - My Telerik MVC Application</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/kendo")
    @Scripts.Render("~/bundles/bootstrap")

	
    <script>
        kendo.culture("ru-RU");
    </script>

</head>
<body>
    <div class="page">

        <header>
            <div id="user" style="display:inline-block; background-color: #696969; font-weight: bold; float:right; height:100%; padding: 16px 15px 14px;">
                <h3>@(((FilesConverting.WebUI.Models.CustomPrincipal)User).FIO)</h3>
                
            </div>
            <h1>МСК ЕВ V</h1>
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer></footer>
    </div>
</body>
</html>
